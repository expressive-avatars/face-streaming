{
  "name": "face-streaming-client",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"yarn dev:app\" \"yarn dev:server\" \"yarn dev:lib\" \"yarn serve:lib\"",
    "tunnel": "ngrok start --all --config ngrok.yaml",
    "dev:app": "vite",
    "dev:server": "NODE_ENV=development nodemon server/index.js",
    "dev:lib": "LIBRARY_MODE=true vite build --watch --mode=development",
    "build": "yarn build:app && yarn build:lib",
    "build:app": "vite build",
    "build:lib": "LIBRARY_MODE=true vite build",
    "serve:lib": "live-server _lib --port=4000 --no-browser --cors --quiet",
    "preview": "live-server dist --no-browser --cors"
  },
  "dependencies": {
    "@react-three/drei": "^8.6.1",
    "@react-three/fiber": "^7.0.24",
    "@react-three/xr": "^3.2.0",
    "express": "^4.17.1",
    "jwt-decode": "^3.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "socket.io": "^4.3.1",
    "socket.io-client": "^4.3.2",
    "suspend-react": "^0.0.8",
    "three": "^0.136.0",
    "twind": "^0.16.16",
    "valtio": "^1.2.9"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.1.4",
    "autoprefixer": "^10.4.2",
    "concurrently": "^6.3.0",
    "live-server": "^1.2.1",
    "ngrok": "^4.2.2",
    "nodemon": "^2.0.14",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.15",
    "vite": "^2.7.12"
  }
}
