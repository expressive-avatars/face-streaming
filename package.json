{
  "name": "face-streaming-client",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"yarn dev:app\" \"yarn dev:server\" \"yarn dev:lib\" \"yarn serve:lib\"",
    "tunnel": "ngrok start --all --config ngrok.yaml",
    "dev:app": "vite",
    "dev:server": "NODE_ENV=development nodemon server/index.js",
    "dev:lib": "LIBRARY_MODE=true vite build --watch --mode=development",
    "build": "concurrently \"yarn build:app\" \"yarn build:lib\"",
    "build:app": "vite build",
    "build:lib": "LIBRARY_MODE=true vite build",
    "serve:lib": "live-server dist --port=4000 --no-browser --cors --quiet"
  },
  "dependencies": {
    "@react-three/drei": "^7.17.1",
    "@react-three/fiber": "^7.0.17",
    "@react-three/xr": "^3.2.0",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "socket.io": "^4.3.1",
    "socket.io-client": "^4.3.2",
    "three": "^0.133.1",
    "valtio": "^1.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.0.5",
    "concurrently": "^6.3.0",
    "live-server": "^1.2.1",
    "nodemon": "^2.0.14",
    "vite": "^2.6.14"
  }
}
