{
  "name": "face-streaming-client",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"yarn dev:frontend\" \"yarn dev:backend\" \"yarn dev:hubs\" \"yarn serve:hubs\"",
    "dev:frontend": "vite",
    "dev:backend": "nodemon server/index.js",
    "tunnel": "ngrok start --all --config ngrok.yaml",
    "build": "vite build",
    "dev:hubs": "LIBRARY_MODE=true vite build --watch",
    "serve:hubs": "live-server dist --port=4000 --cors --no-browser --quiet"
  },
  "dependencies": {
    "@react-three/drei": "^7.17.1",
    "@react-three/fiber": "^7.0.17",
    "@react-three/xr": "^3.2.0",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "socket.io": "^4.3.1",
    "socket.io-client": "^4.3.2",
    "three": "^0.133.1",
    "valtio": "^1.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.0.5",
    "concurrently": "^6.3.0",
    "live-server": "^1.2.1",
    "nodemon": "^2.0.14",
    "vite": "^2.6.10"
  }
}
